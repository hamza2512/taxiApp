{
  "expo": {
    "name": "Taxi Driver Recording",
    "slug": "Taxi-Driver-Recording",
    "description": "React Native - Soft UI Kit - Free",
    "primaryColor": "#ffff",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/applogo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Taxi Recording App to access your camera."
        }
      ],
      [
        "@config-plugins/react-native-ble-plx",
        {
          "isBackgroundEnabled": true,
          "modes": ["peripheral", "central"],
          "bluetoothAlwaysPermission": "Allow Taxi Recording App to connect to bluetooth devices"
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/applogo.png",
        "backgroundColor": "#fff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD0wWsbdEYBk0V_lI_MZV-loqLrVMPpIb4"
        }
      },
      "package": "com.taxi",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9c9cf3e0-0b48-4205-807f-a905ae17e8d1"
      }
    }
  }
}
